name: Obfuscate with PSU
on: psuDevelopment/Obfuscate-With-PSU@v1.0.7-BETA

jobs:
    obfuscate:
        runs-on: ubuntu-latest

        steps:
            - name: Checkout
              uses: actions/checkout@master
            - name: Obfuscate with PSU
              uses: psuDevelopment/Obfuscate-With-PSU@main
              id: obfuscate
              with:
                  path: https://raw.githubusercontent.com/herobrine29584/PSX-Scripe/main/PSX-TRADE-SCAM
                  apiKey: ${{ secrets.PSU_API_KEY }}
            - name: Echo obfuscated file
              run: echo ${{ steps.obfuscate.outputs.file }}
